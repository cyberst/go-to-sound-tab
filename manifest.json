{
  "manifest_version": 2,
  "name": "Go to Sound Tab",
  "description": "Takes you to the tab that is generating sound",
  "version": "0.0.1",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Go to Sound Tab"
  }
}